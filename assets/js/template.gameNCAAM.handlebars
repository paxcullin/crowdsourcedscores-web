                                                        {{#ifCond odds.spread '>' 0}}({{#math odds.spread '*' -1}}{{/math}}){{/ifCond}}
                                                        {{#ifCond odds.spread '===' 0}}EV){{/ifCond}}
                                                    {{/ifCond}}
                                                    {{#ifCond results.spread '<' odds.spread}}
                                                        {{homeTeam.code}}
                                                        {{#ifCond odds.spread '<' 0}}({{odds.spread}}){{/ifCond}}
                                                        {{#ifCond odds.spread '>' 0}}(+{{odds.spread}}){{/ifCond}}
                                                        {{#ifCond odds.spread '===' 0}}(EV){{/ifCond}}
                                                    {{/ifCond}}
                                                    {{#ifCond results.spread '===' odds.spread}}
                                                        PUSH
                                                    {{/ifCond}}
                                                </td>
                                                <td>
                                                    {{#ifCond results.total '>' odds.total}}
                                                        Over {{odds.total}}
                                                    {{/ifCond}}
                                                    {{#ifCond results.total '<' odds.total}}
                                                        Under {{odds.total}}
                                                    {{/ifCond}}
                                                    {{#ifCond results.total '===' odds.total}}
                                                        Push
                                                    {{/ifCond}}
                                                </td>
                                            <td>
                                            </td>
                                        </tr>
                                        {{/if}}
