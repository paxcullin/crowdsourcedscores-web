<div class="card prediction" style="margin-bottom:10px;min-width: 293px;" data-game-id="{{{gameId}}}"
    data-game-year="{{{year}}}"
    data-game-sport="{{{sport}}}"
    data-game-week="{{{gameWeek}}}"
    data-home-code="{{{homeTeam.code}}}"
    data-home-name="{{{homeTeam.fullName}}}" data-home-short="{{{homeTeam.shortName}}}"
    data-away-code="{{{awayTeam.code}}}"
    data-away-name="{{{awayTeam.fullName}}}" data-away-short="{{{awayTeam.shortName}}}"
    {{#if prediction.awayTeam.score}} data-predicted="true" {{/if}}
    style="display: inline-block;">
    <div class="panel-group" id="{{{gameId}}}-accordion" role="tablist" aria-multiselectable="true">
        <div class="panel panel-default">
            <div class="panel-heading" role="tab" id="{{{awayTeam.code}}}-{{{homeTeam.code}}}-heading">
                <h4 class="panel-title">
                    <a role="button" data-toggle="collapse" data-parent="#{{{gameId}}}-accordion" href="#{{{awayTeam.code}}}-{{{homeTeam.code}}}" aria-expanded="true" aria-controls="{{{gameId}}}">
                        {{awayTeam.fullName}} at {{homeTeam.fullName}}                        
                        {{#ifCond prediction.total '===' odds.total}}
                            <i class="fas fa-exclamation-triangle total"></i>
                        {{/ifCond}}
                        {{#ifCond prediction.spread '===' odds.spread}}
                            <i class="fas fa-exclamation-triangle spread"></i>
                        {{/ifCond}}
                    </a>

                </h4>
            </div>
            <div id="{{{awayTeam.code}}}-{{{homeTeam.code}}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="{{{awayTeam.code}}}-{{{homeTeam.code}}}-heading">
                <div class="panel-body">
                    <div class="content">
                        <div class="tab-content text-center">
                            <div class="tab-pane active">
                                <div class="prediction-message" data-game-id="{{{gameId}}}"></div>
                                <div>
                                    <div class="form-group">
                                        <div class="gameHeader{{#if prediction}} gameHeaderPredicted{{/if}}{{#unless prediction}} gameHeaderNotPredicted{{/unless}}">
                                            <div>{{awayTeam.fullName}} vs. {{homeTeam.fullName}}</div>
                                            <div style="margin: 0 auto">{{startDateTime}}<br/>{{location}}
                                            {{#if results}}
                                                <br><span class="finalRow" style="float: right">FINAL</span>
                                            {{/if}}
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>


                                <table class="table">
                                    <thead>
                                        <tr class="predictionHeaderRow">
                                            <th><!--predictor--></th>
                                            <th>{{awayTeam.rank}} {{awayTeam.code}}</th>
                                            <th>{{homeTeam.rank}} {{homeTeam.code}}</th>
                                            {{#ifCond prediction '||' results}}
                                                <th>Side</th>
                                                <th>Total</th>
                                            {{/ifCond}}
                                            <th>
                                                {{#if results}}
                                                    <span class="hidden-xs">Prediction </span>Score
                                                {{/if}}
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        {{!--User Prediction Row--}}
                                        <tr class="predictionRow">
                                            <td>Me</td>
                                            <td class="away-team-prediction {{#if results}}{{#ifCond results.awayTeam.score '>' results.homeTeam.score}}{{#ifCond prediction.awayTeam.score '>' prediction.homeTeam.score}} alert alert-success{{/ifCond}}{{/ifCond}}{{/if}}" id="{{{awayTeam.code}}}-prediction">
                                                {{#ifCond status '===' 'notStarted'}}
                                                    <input type="number" min="0" max="150" class="form-control predictionInput" placeholder="##"
                                                    value="{{prediction.awayTeam.score}}">
                                                {{/ifCond}}
                                                {{#ifCond status '!==' 'notStarted'}}
                                                    {{#ifCond prediction.awayTeam.score '>' prediction.homeTeam.score}}<i class="fas fa-angle-double-right"></i>{{/ifCond}}{{prediction.awayTeam.score}}
                                                    {{#if results}}
                                                        {{#ifCond prediction.awayTeam.score '===' results.awayTeam.score}}
                                                            <sup><i class="fas fa-bullseye"></i></sup>
                                                        {{/ifCond}}
                                                    {{/if}}
                                                {{/ifCond}}
                                            </td>
                                            <td class="home-team-prediction{{#if results}}{{#ifCond results.homeTeam.score '>' results.awayTeam.score}}{{#ifCond prediction.homeTeam.score '>' prediction.awayTeam.score}} alert alert-success{{/ifCond}}{{/ifCond}}{{/if}}" id="{{{homeTeam.code}}}-prediction">
                                                {{#ifCond status '===' 'notStarted'}}
                                                    <input type="number" min="0" max="150" class="form-control predictionInput" placeholder="##"
                                                    value="{{prediction.homeTeam.score}}">
                                                {{/ifCond}}
                                                {{#ifCond status '!==' 'notStarted'}}
                                                    {{#ifCond prediction.awayTeam.score '<' prediction.homeTeam.score}}<i class="fas fa-angle-double-right"></i>{{/ifCond}}
                                                    {{prediction.homeTeam.score}}
                                                    {{#if results}}
                                                        {{#ifCond prediction.homeTeam.score '===' results.homeTeam.score}}
                                                            <sup><i class="fas fa-bullseye"></i></sup>
                                                        {{/ifCond}}
                                                    {{/if}}
                                                {{/ifCond}}
                                            </td>
                                            {{#ifCond prediction '||' results}}
                                                <td{{#if prediction.results}}{{#ifCond prediction.results.spread.correct '===' 1}} class="alert alert-success"{{/ifCond}}{{/if}}>
                                                    {{#ifCond prediction.spread '>' odds.spread}}
                                                        {{awayTeam.code}}
                                                        {{#ifCond odds.spread '<' 0}}(+{{#math odds.spread '*' -1}}{{/math}}){{/ifCond}}
                                                        {{#ifCond odds.spread '>' 0}}({{#math odds.spread '*' -1}}{{/math}}){{/ifCond}}
                                                        {{#ifCond odds.spread '===' 0}}EV){{/ifCond}}
                                                    {{/ifCond}}
                                                    {{#ifCond prediction.spread '<' odds.spread}}
                                                        {{homeTeam.code}}
                                                        {{#ifCond odds.spread '<' 0}}({{odds.spread}}){{/ifCond}}
                                                        {{#ifCond odds.spread '>' 0}}(+{{odds.spread}}){{/ifCond}}
                                                        {{#ifCond odds.spread '===' 0}}(EV){{/ifCond}}
                                                    {{/ifCond}}
                                                    {{#ifCond prediction.spread '===' odds.spread}}
                                                        PUSH<sup><i class="fas fa-exclamation-triangle spread"></i><span class="warningTextSpread">Your spread matches the odds spread.</span></sup>
                                                    {{/ifCond}}
                                                </td>
                                                <td {{#if prediction.results}}{{#ifCond prediction.results.total.correct '===' 1}} class="alert alert-success"{{/ifCond}}{{/if}}>
                                                    {{#ifCond prediction.total '>' odds.total}}
                                                        Over {{odds.total}}
                                                    {{/ifCond}}
                                                    {{#ifCond prediction.total '<' odds.total}}
                                                        Under {{odds.total}}
                                                    {{/ifCond}}
                                                    {{#ifCond prediction.total '===' odds.total}}
                                                        Push<sup><i class="fas fa-exclamation-triangle spread"></i><span class="warningTextSpread">Your total matches the odds total.</span></sup>
                                                    {{/ifCond}}
                                                </td>
                                            {{/ifCond}}
                                            <td>
                                                {{#if results}}
                                                    {{#ifCond prediction.predictionScore '>' -1}}
                                                        <div class="predictionScore myResultsRow">{{prediction.predictionScore}} <i class="fas fa-info-circle" data-toggle="modal" data-target="#predictionScoreModal"></i></div>
                                                    {{/ifCond}}

                                                {{else}}
                                                    {{#if userAuthenticated}}
                                                        {{#dateCheck startDateTime}}
                                                            <button class="prediction-btn btn btn-info">{{#if prediction}}Update{{else}}Predict{{/if}}</button>
                                                        {{/dateCheck}}
                                                    {{else}}
                                                        {{#dateCheck startDateTime}}
                                                            <button type="button" class="btn btn-info" onclick="ga('send','event','login','predictButtonClicked');window.location=('https://crowdsourcedscores.auth.us-west-2.amazoncognito.com/login?response_type=code&client_id=2n15lhk845sucm0k4fejjqcbev&redirect_uri=https://{{getWindowLocation}}')">Log in to Predict</button>
                                                        {{/dateCheck}}
                                                    {{/if}}
                                                {{/if}}
                                            </td>
                                        </tr>
                                        {{!--Crowd Prediction Row--}}
                                        {{#ifCond prediction '||' results}}
                                            {{#if crowd}}
                                            <tr class="predictionRow">
                                                <td>Crowd</td>
                                                <td class="away-team-prediction {{#if results}}{{#ifCond results.awayTeam.score '>' results.homeTeam.score}}{{#ifCond crowd.awayTeam.score '>' crowd.homeTeam.score}} alert alert-success{{/ifCond}}{{/ifCond}}{{/if}}" id="{{{awayTeam.code}}}-prediction">
                                                    
                                                    {{#ifCond crowd.awayTeam.score '>' crowd.homeTeam.score}}<i class="fas fa-angle-double-right"></i>{{/ifCond}}{{crowd.awayTeam.score}}
                                                    {{#if results}}
                                                        {{#ifCond crowd.awayTeam.score '===' results.awayTeam.score}}
                                                            <sup><i class="fas fa-bullseye"></i></sup>
                                                        {{/ifCond}}
                                                    {{/if}}
                                                </td>
                                                <td class="home-team-prediction{{#if results}}{{#ifCond results.homeTeam.score '>' results.awayTeam.score}}{{#ifCond prediction.homeTeam.score '>' prediction.awayTeam.score}} alert alert-success{{/ifCond}}{{/ifCond}}{{/if}}" id="{{{homeTeam.code}}}-prediction">
                                                        {{#ifCond crowd.awayTeam.score '<' crowd.homeTeam.score}}<i class="fas fa-angle-double-right"></i>{{/ifCond}}
                                                        {{crowd.homeTeam.score}}
                                                        {{#if results}}
                                                            {{#ifCond crowd.homeTeam.score '===' results.homeTeam.score}}
                                                                <sup><i class="fas fa-bullseye"></i></sup>
                                                            {{/ifCond}}
                                                        {{/if}}
                                                </td>
                                                {{#ifCond crowd '||' results}}
                                                    <td {{#if crowd.results}}{{#ifCond crowd.results.spread.correct '===' 1}} class="alert alert-success"{{/ifCond}}{{/if}}>
                                                        {{#ifCond crowd.spread '>' odds.spread}}
                                                            {{awayTeam.code}}
                                                            {{#ifCond odds.spread '<' 0}}(+{{#math odds.spread '*' -1}}{{/math}}){{/ifCond}}
                                                            {{#ifCond odds.spread '>' 0}}({{#math odds.spread '*' -1}}{{/math}}){{/ifCond}}
                                                            {{#ifCond odds.spread '===' 0}}EV){{/ifCond}}
                                                        {{/ifCond}}
                                                        {{#ifCond crowd.spread '<' odds.spread}}
                                                            {{homeTeam.code}}
                                                            {{#ifCond odds.spread '<' 0}}({{odds.spread}}){{/ifCond}}
                                                            {{#ifCond odds.spread '>' 0}}(+{{odds.spread}}){{/ifCond}}
                                                            {{#ifCond odds.spread '===' 0}}(EV){{/ifCond}}
                                                        {{/ifCond}}
                                                        {{#ifCond crowd.spread '===' odds.spread}}
                                                            PUSH
                                                        {{/ifCond}}
                                                    </td>
                                                    <td {{#if crowd.results}}{{#ifCond crowd.results.total.correct '===' 1}} class="alert alert-success"{{/ifCond}}{{/if}}>
                                                        {{#ifCond crowd.total '>' odds.total}}
                                                            Over {{odds.total}}
                                                        {{/ifCond}}
                                                        {{#ifCond crowd.total '<' odds.total}}
                                                            Under {{odds.total}}
                                                        {{/ifCond}}
                                                        {{#ifCond crowd.total '===' odds.total}}
                                                            Push
                                                        {{/ifCond}}
                                                    </td>
                                                {{/ifCond}}
                                                <td>
                                                    {{#if crowd.results}}
                                                        <div class="predictionScore crowdResultsRow">{{crowd.results.predictionScore}}</div>
                                                    {{/if}}
                                                </td>
                                            </tr>
                                            {{/if}}
                                        {{/ifCond}}
                                        {{!--Results  Row--}}
                                        {{#if results}}
                                        <tr class="predictionRow">
                                            <td>Actual</td>
                                            <td class="away-team-results" id="{{{awayTeam.code}}}-results">
                                                {{#ifCond results.awayTeam.score '>' results.homeTeam.score}}<i class="fas fa-angle-double-right"></i>{{/ifCond}}{{results.awayTeam.score}}
                                            </td>
                                            <td class="home-team-results" id="{{{homeTeam.code}}}-results">
